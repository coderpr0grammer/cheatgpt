{
  "manifest_version": 3,
  "name": "CheatGPT",
  "version": "1.0",
  "description": "A simple Chrome extension.",
  "permissions": [
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://docs.google.com/forms/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ]
}
